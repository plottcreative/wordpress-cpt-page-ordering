{
    "name": "plottcreative/wordpress-cpt-page-ordering",
    "version": "0.1.12",
    "description": "WordPress plugin that allows administrators to easily reorder posts, pages, and custom post types (CPTs) through a simple drag-and-drop interface in the WordPress admin area.",
    "type": "wordpress-plugin",
    "minimum-stability": "stable",
    "keywords": ["wordpress", "plugin", "drag-and-drop", "ordering", "cpt", "menu_order"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Ewan Lockwood",
            "email": "ewan@plott.co.uk"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "PlottOs\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "installer-name": "wordpress-cpt-page-ordering"
    }
}   